<?xml version="1.0"?>
<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
    <Frame name="TG3AuraTemplate" virtual="true">
        <Frames>
            <Cooldown parentKey="Cooldown" setAllPoints="true" reverse="true" drawEdge="true" inherits="CooldownFrameTemplate"/>
        </Frames>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="Texture" setAllPoints="true"/>
            </Layer>
            <Layer level="OVERLAY">
                <FontString parentKey="Count" setAllPoints="true" justifyH="RIGHT" justifyV="BOTTOM"/>
            </Layer>
        </Layers>
    </Frame>

    <Frame name="TG3BuffTemplate" mixin="TG3BuffMixin" inherits="TG3AuraTemplate" virtual="true"/>
    <Frame name="TG3DebuffTemplate" mixin="TG3DebuffMixin" inherits="TG3AuraTemplate" virtual="true"/>
</Ui>
